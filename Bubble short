#include<stdio.h>
#include<conio.h>
void swap(int*a,int*b)
{
	int temp;
	temp=*a;
     *a=*b;
	 *b=temp;	
}
void bubbleshort(int narr[],int n)
{
	int i,j;
	for(i=0;i<n-1;i++)
	{
	for(j=0;j<n-i-1;j++)
	{
    if(arr[j]>arr[j+1])
